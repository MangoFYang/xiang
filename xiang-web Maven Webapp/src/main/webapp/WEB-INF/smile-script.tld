		<!-- 继承 osinglebox -->
		<attribute>
			<description><![CDATA[弹出框的标题。]]></description>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[弹出框的宽度。]]></description>
			<name>odialogWidth</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
