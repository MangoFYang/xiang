		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[ ocheckbox的标签。]]></description>
			<name>label</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ 默认值。 ]]></description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ 默认是否选中。 ]]></description>
			<name>checked</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ 事件: 选中时触发。 ]]></description>
			<name>check</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ 事件: 取消选中时触发。 ]]></description>
			<name>uncheck</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<!-- 继承 抽象的UI标签 -->
		<attribute>
			<description><![CDATA[渲染到对应的HTML元素。]]></description>
			<name>renderTo</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<!-- ocheckbox 结束 -->
	
	<!-- oradio 开始 -->
	<tag>
		<description><![CDATA[oradio组件。]]></description>
		<name>oradio</name>
		<tag-class>smile.views.jstl.ui.script.ORadioTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[ocheckbox的标签。]]></description>
