			<name>rowList</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[如果为true，jqGrid显示记录信息，显示内容可以调整emptyrecords、recordtext。]]></description>
			<name>viewrecords</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[启用多选，会产生新列checkbox用于多选。]]></description>
			<name>multiselect</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[会产生新列显示行号。]]></description>
			<name>rownumbers</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[初始化隐藏grid，不请求数据，值显示caption层，当点击显示/隐藏按钮时，显示grid的并请求数据。]]></description>
			<name>hiddengrid</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[当caption不为""时，隐藏grid。]]></description>
			<name>hidegrid</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[排序列名。]]></description>
			<name>sortname</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
