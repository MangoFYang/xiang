			<name>checked</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ 事件: 选中时触发。 ]]></description>
			<name>check</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ 事件: 取消选中时触发。 ]]></description>
			<name>uncheck</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<!-- 继承 抽象的UI标签 -->
		<attribute>
			<description><![CDATA[渲染到对应的HTML元素。]]></description>
			<name>renderTo</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
