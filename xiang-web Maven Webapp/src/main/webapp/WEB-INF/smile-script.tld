<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<tlib-version>1.0</tlib-version>
	<short-name>s</short-name>
	<uri>/smile/script</uri>

	<!-- output 开始 -->
	<tag>
		<description><![CDATA[var bar = ...]]></description>
		<name>output</name>
		<tag-class>smile.views.jstl.ui.script.OutputTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[变量名。]]></description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[变量值，需要JSON序列化的Java对象，可以使用EL表达式，如：${ foo }。]]></description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- output 结束 -->

	<!-- script 开始 -->
	<tag>
		<description><![CDATA[<script type="text/javascript">$(function() {...});</script>]]></description>
		<name>script</name>
		<tag-class>smile.views.jstl.ui.script.ScriptTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[引用的JS文件。]]></description>
			<name>src</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<!-- script 结束 -->
	
	<!-- olocation 开始 -->
	<tag>
		<description><![CDATA[olocation组件。]]></description>
		<name>olocation</name>
		<tag-class>smile.views.jstl.ui.script.OLocationTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[['权限管理', '角色管理']]]></description>
			<name>location</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[ >> ]]></description>
			<name>separator</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<!-- 继承 抽象的UI标签 -->
		<attribute>
			<description><![CDATA[渲染到对应的HTML元素。]]></description>
			<name>renderTo</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<!-- olocation 结束 -->

	<!-- ocheckbox 开始 -->
	<tag>
		<description><![CDATA[ ocheckbox组件。]]></description>
		<name>ocheckbox</name>
